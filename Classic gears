--Build
_G.Build = game:GetObjects('rbxassetid://16201628')[1]
_G.Build.Parent = game.Players.LocalPlayer.Backpack
_G.Build.Equipped:Connect(function()
local Sound = Instance.new("Sound") 
local Id = "12221944"
Sound.Parent = game.Workspace
Sound.SoundId = "rbxassetid://"..Id
Sound.Playing = true
Sound.Looped = false
Sound.Volume = 2.5
end)
